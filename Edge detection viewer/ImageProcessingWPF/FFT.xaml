        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ImageProcessingWPF"
        mc:Ignorable="d"
        Title="FFT" Height="200" Width="300">
    <StackPanel Orientation="Vertical" Width="300">



        <Button x:Name="lena"  
            FontWeight="Bold" FontSize="14" Margin="15" Width="150" Height="50" IsDefault="True" Click="Lena">lena
        </Button>
        <Button x:Name="mri"   
            FontWeight="Bold" FontSize="14" Margin="15" Width="150" Height="50" IsDefault="True" Click="Mri" >Mri
        </Button>
       
    </StackPanel>
</Window>
